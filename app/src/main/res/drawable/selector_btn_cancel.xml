<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="#ffededed" android:endColor="#ffededed" android:angle="270.0" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffededed" android:endColor="#ffededed" android:angle="270.0" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffc2c2c2" android:endColor="#ffc2c2c2" android:angle="270.0" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>